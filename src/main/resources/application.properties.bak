server.address=0.0.0.0
server.port=22000
management.server.address=${server.address}
management.server.port=-1
management.server.add-application-context-header=true

server.tomcat.remote-ip-header=X-Forwarded-For

spring.datasource.url=jdbc:mysql://127.0.0.1:3306/move_car?useSSL=false&characterEncoding=utf-8&serverTimezone=UTC
spring.datasource.username=root
spring.datasource.password=root
spring.datasource.hikari.maximum-pool-size=50
spring.datasource.hikari.minimum-idle=5
spring.datasource.hikari.connection-timeout=10000



spring.thymeleaf.prefix=classpath:/views/
spring.thymeleaf.suffix=.html


spring.mvc.static-path-pattern=/static/**
